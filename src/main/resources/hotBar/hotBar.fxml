<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="15.0" xmlns="http://javafx.com/javafx/14" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scanner.controller.hotBar.HotBarController">
    <fx:define/>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickNew" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/document.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickOpen" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/folder.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickSave" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/save.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickCut" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/scissors.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickCopy" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/copy.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickPaste" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/arquivo.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickCompile" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/settings.png" />
    </ImageView>
    <ImageView fitHeight="25.0" fitWidth="40.0" onMouseClicked="#onClickExecute" pickOnBounds="true" preserveRatio="true">
        <Image url="@../icons/running.png" />
    </ImageView>
   <padding>
      <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
   </padding>
</HBox>
